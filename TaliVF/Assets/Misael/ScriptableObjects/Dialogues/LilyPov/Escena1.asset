  - <Character>k__BackingField: {fileID: 11400000, guid: 905b5c51828720a42b7682bd860dbec3, type: 2}
    dialogueLine: "No tenemos otra opci\xF3n"
    isPerson1: 0
    Person1: {fileID: 21300000, guid: 7984f644e8b2aaf4aa4ae9241f3cb698, type: 3}
    Person2: {fileID: 520602433, guid: 3f635272c85aeee4bb32c4a3eed58e6f, type: 3}
    personajeInvisible: 0
    hideImagePerson1: 0
