  - <Character>k__BackingField: {fileID: 11400000, guid: 905b5c51828720a42b7682bd860dbec3, type: 2}
    dialogueLine: "Como si hubiera algo que valga la pena conservar\u2026\r\nV\xE1monos.\r\n\r\n"
    isPerson1: 0
    Person1: {fileID: 21300000, guid: 877c806b7d0ea79428060444e96c25b8, type: 3}
    Person2: {fileID: 1409860228, guid: d454c6ab5c899e042ae873a5e14f35bb, type: 3}
